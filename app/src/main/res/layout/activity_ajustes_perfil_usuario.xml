<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".Ajustes_PerfilUsuario">

    <TextView
        android:id="@+id/textView53"
        android:layout_width="414dp"
        android:layout_height="56dp"
        android:background="@color/teal_700"
        android:fontFamily="sans-serif-medium"
        android:paddingLeft="20sp"
        android:paddingTop="10sp"
        android:text="@string/PerfilDatos"
        android:textAlignment="textStart"
        android:textColor="@color/white"
        android:textSize="24sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_nombreperfil"
        android:layout_width="203dp"
        android:layout_height="42dp"
        android:layout_marginTop="48dp"
        android:background="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/textView53"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/textView53"
        app:layout_constraintTop_toBottomOf="@+id/textView53" />

    <TextView
        android:id="@+id/textView55"
        android:layout_width="115dp"
        android:layout_height="22dp"
        android:layout_marginTop="24dp"
        android:background="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/tv_nombreperfil"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/tv_nombreperfil"
        app:layout_constraintTop_toBottomOf="@+id/tv_nombreperfil" />

    <TextView
        android:id="@+id/textView56"
        android:layout_width="350dp"
        android:layout_height="45dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="144dp"
        android:layout_marginEnd="15dp"
        android:background="@color/white"
        android:drawableRight="@drawable/ic_baseline_arrow_forward_ios_24"
        android:fontFamily="sans-serif-medium"
        android:onClick="Ajustes_PerfilUsuario"
        android:paddingLeft="20sp"
        android:paddingTop="10sp"
        android:text="@string/Perfildatos2"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/textView55"
        app:layout_constraintStart_toStartOf="@+id/textView55"
        app:layout_constraintTop_toBottomOf="@+id/textView55" />

    <ImageView
        android:id="@+id/imageView112"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginBottom="40dp"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/atras" />
</androidx.constraintlayout.widget.ConstraintLayout>