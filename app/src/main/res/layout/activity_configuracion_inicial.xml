<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ConfiguracionInicial">

    <EditText
        android:id="@+id/nombre_usuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/tv_nombre_usuario"
        android:ems="10"
        android:hint="Nombre"
        android:inputType="textPersonName"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.222"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.067" />

    <EditText
        android:id="@+id/Calle_usuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/tv_calle_usuario"
        android:ems="10"
        android:hint="Calle"
        android:inputType="textPersonName"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/telefono_usuario"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/telefono_usuario"
        app:layout_constraintTop_toBottomOf="@+id/telefono_usuario"
        app:layout_constraintVertical_bias="0.083" />

    <EditText
        android:id="@+id/numCalle_usuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Numero"
        android:inputType="number"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/Calle_usuario"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/Calle_usuario"
        app:layout_constraintTop_toBottomOf="@+id/Calle_usuario"
        app:layout_constraintVertical_bias="0.072" />

    <EditText
        android:id="@+id/telefono_usuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/telefono_usuario"
        android:ems="10"
        android:hint="Telefono"
        android:inputType="phone"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/email_usuario"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/email_usuario"
        app:layout_constraintTop_toBottomOf="@+id/email_usuario"
        app:layout_constraintVertical_bias="0.061" />

    <EditText
        android:id="@+id/email_usuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/email_usuario"
        android:ems="10"
        android:hint="E-mail"
        android:inputType="textEmailAddress"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/apellido_usuario"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/apellido_usuario"
        app:layout_constraintTop_toBottomOf="@+id/apellido_usuario"
        app:layout_constraintVertical_bias="0.052" />

    <EditText
        android:id="@+id/apellido_usuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/tv_apellido_usuario"
        android:ems="10"
        android:hint="Apellido"
        android:inputType="textPersonName"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/nombre_usuario"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/nombre_usuario"
        app:layout_constraintTop_toBottomOf="@+id/nombre_usuario"
        app:layout_constraintVertical_bias="0.039" />

    <EditText
        android:id="@+id/numDto_usuario"
        android:layout_width="124dp"
        android:layout_height="47dp"
        android:contentDescription="@string/tv_Departamento_usuario"
        android:ems="10"
        android:hint="Depto."
        android:inputType="textPersonName"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.668"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/numCalle_usuario"
        app:layout_constraintVertical_bias="0.189" />

    <EditText
        android:id="@+id/piso_usuario"
        android:layout_width="124dp"
        android:layout_height="47dp"
        android:contentDescription="@string/tv_Piso_usuario"
        android:ems="10"
        android:hint="Piso"
        android:inputType="textPersonName"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.135"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/numCalle_usuario"
        app:layout_constraintVertical_bias="0.189" />

    <ImageButton
        android:id="@+id/IB_BIEN"
        android:layout_width="92dp"
        android:layout_height="94dp"
        android:contentDescription="@string/IB_BIEN"
        android:onClick="Bien"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.899"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/numDto_usuario"
        app:layout_constraintVertical_bias="0.472"
        app:srcCompat="@drawable/bien" />

</androidx.constraintlayout.widget.ConstraintLayout>