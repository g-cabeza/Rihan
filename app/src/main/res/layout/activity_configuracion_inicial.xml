<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ConfiguracionInicial">

    <EditText
        android:id="@+id/nombre_usuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="40dp"
        android:contentDescription="@string/tv_nombre_usuario"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/apellido_usuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:contentDescription="@string/tv_apellido_usuario"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/tv_apellido_usuario"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nombre_usuario"
        tools:text="@string/tv_apellido_usuario" />

    <EditText
        android:id="@+id/Calle_usuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:contentDescription="@string/tv_calle_usuario"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/tv_calle_usuario"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/apellido_usuario"
        tools:text="@string/tv_calle_usuario" />

    <EditText
        android:id="@+id/numCalle_usuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:inputType="number"
        android:text="@string/tvn_numerocalle_usuario"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Calle_usuario"
        tools:text="@string/tvn_numerocalle_usuario" />

    <EditText
        android:id="@+id/piso_usuario"
        android:layout_width="124dp"
        android:layout_height="47dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:contentDescription="@string/tv_Piso_usuario"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/tv_Piso_usuario"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/numCalle_usuario"
        tools:text="@string/tv_Piso_usuario" />

    <EditText
        android:id="@+id/numDto_usuario"
        android:layout_width="124dp"
        android:layout_height="47dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        android:contentDescription="@string/tv_Departamento_usuario"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/tv_Departamento_usuario"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/piso_usuario"
        app:layout_constraintTop_toBottomOf="@+id/numCalle_usuario"
        tools:text="@string/tv_Departamento_usuario" />

    <EditText
        android:id="@+id/telefono_usuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:contentDescription="@string/telefono_usuario"
        android:ems="10"
        android:inputType="phone"
        android:text="@string/telefono_usuario"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/piso_usuario"
        tools:text="@string/telefono_usuario" />

    <EditText
        android:id="@+id/email_usuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:contentDescription="@string/email_usuario"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:text="@string/email_usuario"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/telefono_usuario"
        tools:text="@string/email_usuario" />

    <ImageButton
        android:id="@+id/IB_BIEN"
        android:layout_width="92dp"
        android:layout_height="94dp"
        android:layout_marginStart="270dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="40dp"
        android:contentDescription="@string/IB_BIEN"
        android:onClick="Bien"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email_usuario"
        app:srcCompat="@drawable/bien" />

</androidx.constraintlayout.widget.ConstraintLayout>